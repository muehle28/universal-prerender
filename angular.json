{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "universal-prerender": {
      "projectType": "application",
      "i18n": {
        "sourceLocale": "de",
        "locales": {
          "en": "src/locale/messages.en.xlf",
          "it": "src/locale/messages.it.xlf"
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "localize": [
              "de"
            ],
            "outputPath": "dist/universal-prerender/browser",
            "outputHashing": "all",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "all": {
              "localize": true
            },
            "en": {
              "localize": [
                "en"
              ]
            },
            "it": {
              "localize": [
                "it"
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "universal-prerender:build",
            "proxyConfig": "proxy.conf.js",
            "aot": true,
            "watch": true,
            "liveReload": true,
            "open": true
          },
          "configurations": {
            "all": {
              "browserTarget": "universal-prerender:build:all"
            },
            "en": {
              "browserTarget": "universal-prerender:build:en"
            },
            "it": {
              "browserTarget": "universal-prerender:build:it"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "universal-prerender:build",
            "outputPath": "src/locale",
            "outFile": "messages.xlf",
            "format": "xlf",
            "ivy": true
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "universal-prerender:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "universal-prerender:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "localize": [
              "de"
            ],
            "outputPath": "dist/universal-prerender/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "all": {
              "localize": true
            },
            "en": {
              "localize": [
                "en"
              ]
            },
            "it": {
              "localize": [
                "it"
              ]
            },
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "universal-prerender:build",
            "serverTarget": "universal-prerender:server"
          },
          "configurations": {
            "all": {
              "browserTarget": "universal-prerender:build:production,all",
              "serverTarget": "universal-prerender:server:production,all"
            },
            "en": {
              "browserTarget": "universal-prerender:build:production,en",
              "serverTarget": "universal-prerender:server:production,en"
            },
            "it": {
              "browserTarget": "universal-prerender:build:production,it",
              "serverTarget": "universal-prerender:server:production,it"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "universal-prerender:build:production",
            "serverTarget": "universal-prerender:server:production",
            "routesFile": "./routes.txt"
          },
          "configurations": {
            "production": {},
            "all": {
              "browserTarget": "universal-prerender:build:production,all",
              "serverTarget": "universal-prerender:server:production,all"
            },
            "en": {
              "browserTarget": "universal-prerender:build:production,en",
              "serverTarget": "universal-prerender:server:production,en"
            },
            "it": {
              "browserTarget": "universal-prerender:build:production,it",
              "serverTarget": "universal-prerender:server:production,it"
            }
          }
        }
      }
    }
  },
  "defaultProject": "universal-prerender"
}
