{
  "name": "universal-prerender",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:it": "ng serve --configuration=it",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr:all": "ng build --configuration=production,all && ng run universal-prerender:server --configuration=production,all && npm run serve:ssr:all",
    "dev:ssr": "ng run universal-prerender:serve-ssr",
    "dev:ssr:en": "ng run universal-prerender:serve-ssr --configuration=en",
    "dev:ssr:it": "ng run universal-prerender:serve-ssr --configuration=it",
    "serve:ssr": "node dist/universal-prerender/server/de/main.js",
    "serve:ssr:all": "node main.server.js",
    "serve:ssr:en": "node dist/universal-prerender/server/en/main.js",
    "serve:ssr:it": "node dist/universal-prerender/server/it/main.js",
    "build:ssr": "ng build --prod --configuration=production && ng run universal-prerender:server",
    "build:ssr:all": "ng build --prod --configuration=production,all && ng run universal-prerender:server --configuration=all",
    "build:ssr:en": "ng build --prod --configuration=production,en && ng run universal-prerender:server --configuration=en",
    "build:ssr:it": "ng build --prod --configuration=production,it && ng run universal-prerender:server --configuration=it",
    "prerender": "ng run universal-prerender:prerender",
    "prerender:all": "ng run universal-prerender:prerender --configuration=all",
    "prerender:en": "ng run universal-prerender:prerender --configuration=en",
    "prerender:it": "ng run universal-prerender:prerender --configuration=it",
    "extract-i18n": "ng run universal-prerender:extract-i18n",
    "serve": "npx lite-server -c ./bs-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.3",
    "@angular/common": "^11.0.3",
    "@angular/compiler": "^11.0.3",
    "@angular/core": "^11.0.3",
    "@angular/forms": "^11.0.3",
    "@angular/platform-browser": "^11.0.3",
    "@angular/platform-browser-dynamic": "^11.0.3",
    "@angular/platform-server": "^11.0.3",
    "@angular/router": "^11.0.3",
    "@angular/service-worker": "~11.0.0",
    "@nguniversal/express-engine": "^11.0.1",
    "express": "^4.15.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular/cli": "^11.0.3",
    "@angular/compiler-cli": "^11.0.3",
    "@angular/localize": "^11.0.3",
    "@nguniversal/builders": "^11.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.6.2",
    "@types/node": "^12.19.8",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
